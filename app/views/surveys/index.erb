<div class='container'>
  <h3>Check out our surveys:</h3>
  <div class="list-group">
    <% @surveys.each do |survey| %>
      <a class="list-group-item" href=<%= "/surveys/#{survey.id}" %>><%= survey.name %> <%= "(created #{survey.nice_date} by #{survey.owner})" %></a>
    <% end %>
  </div>
</div>
